{
  "name": "streamiterator",
  "version": "1.3.0",
  "description": "converts ReadableStream into AsyncIterator",
  "keywords": [
    "iterable",
    "stream",
    "ReadableStream",
    "async",
    "iterator",
    "AsyncIterator"
  ],
  "repository": "https://github.com/vadzim/streamiterator",
  "bugs": {
    "url": "https://github.com/vadzim/streamiterator/issues"
  },
  "contributors": [],
  "main": "distr/streamiterator.js",
  "author": "Vadzim Zie≈Ñka <developer@vadzim.info>",
  "scripts": {
    "build": "rm distr/*; babel source --out-dir distr && cd distr && (for f in $(find | grep -E '.js$'); do if grep @flow ../source/$f >/dev/null; then cp ../source/$f $f.flow; fi; done) && cd ..",
    "prettier": "prettier --write $(cat .prettier) 'source/**/*.js' 'example/**/*.js' '*.js'",
    "prepare": "npm run build",
    "test": "jest",
    "go": "npm run prettier && npm run build && npm run test"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-idx": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "flow-bin": "^0.56.0",
    "idx": "^1.5.0",
    "jest": "^21.2.1",
    "prettier": "^1.7.4"
  },
  "dependencies": {
    "@vadzim/callback-to-iterator": "^0.2.1",
    "@vadzim/get-async-iterable": "^0.0.1",
    "babel-runtime": "^6.26.0"
  }
}
